{
  "codeLens.enable": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "go.goplsOptions": {
    "gofumpt": true
  },
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "i": false, // cancel default mapkey
    "<tab>": "actionMenu",

    "h": ["wait", "collapse"],
    "gl": ["wait", "expanded?", "collapse:recursive", "expand:recursive"],
    "<cr>": ["wait", "expanded?", "collapse", "expand"],
    "o": ["wait", "expanded?", "collapse", "expand"],
    "e": "open",
    "s": "open:split",
    "E": "open:vsplit",
    "t": "open:tab",

    "yp": "copyFilepath",
    "yn": "copyFilename",
    "yy": "copyFile",
    "dd": "cutFile",
    "p": "pasteFile",
    "df": "delete",
    "dF": "deleteForever",

    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",

    "zh": "toggleHidden",
    "g<dot>": "toggleHidden",
    "R": "refresh",

    "?": "help",
    "q": "quit",
    "<esc>": "esc",
    "X": "systemExecute",
    "gd": "listDrive",

    "f": "search",
    "F": "searchRecursive",

    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",

    "[c": ["wait", "markPrev:git"],
    "]c": ["wait", "markNext:git"]
  },
  "explorer.autoExpandOptions": ["uncompact"],
  "explorer.previewAction.onHover": "labeling",
  "html.format.indentInnerHtml": true,
  "html.format.wrapAttributes": "force-expand-multiline",
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "typescript.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "java.import.gradle.enabled": true,
  "javascript.suggestionActions.enabled": false,
  "git.signPriority": 1,
  "python.analysis.typeCheckingMode": "off",
  "pyright.disableDiagnostics": false,
  "python.formatting.provider": "black",
}
